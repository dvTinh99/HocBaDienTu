package Model;

public class KhoiHoc {
	String MaKH;
	String TenKH;
	int Ma_school;
	
	KhoiHoc(){}

	public KhoiHoc(String maKH, String tenKH, int ma_school) {
		super();
		MaKH = maKH;
		TenKH = tenKH;
		Ma_school = ma_school;
	}

	public String getMaKH() {
		return MaKH;
	}

	public void setMaKH(String maKH) {
		MaKH = maKH;
	}

	public String getTenKH() {
		return TenKH;
	}

	public void setTenKH(String tenKH) {
		TenKH = tenKH;
	}

	public int getMa_school() {
		return Ma_school;
	}

	public void setMa_school(int ma_school) {
		Ma_school = ma_school;
	}
	
}
